// Test Three
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Records {
  id         Int      @id @default(autoincrement())
  firstName  String
  middleName String
  lastName   String
  gender     String
  email      String
  phone      BigInt
  age        Int
  address    String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
